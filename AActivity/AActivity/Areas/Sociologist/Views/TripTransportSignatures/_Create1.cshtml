@model AActivity.Models.Letter

@{

    var signature1 = Model.TripTransportSignatures.Where(d => d.StatusSignature == 1).FirstOrDefault();


}
@functions{
    bool IsSameSignutre(int userId)
    {
        var cc = ViewBag.signutre as List<AActivity.Models.Signature>;
        foreach (var si in cc)
        {
            if (si.UserId == userId && si.SignatureRole == "وكيل العمادة لشؤون النشاط" && si.Status==true)
            {
                return true;
            }
        }
        return false;
    }
}

@if (signature1 != null)
{
    if (!IsSameSignutre(signature1.Signature.UserId))
    {
        @:عنه
    }
    <img class="" width="90px" height="90px" src="~/img/signatures/@signature1.Signature.SignaturePhoto" />

}